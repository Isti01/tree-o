<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="Styles/MainMenuStyles.uss" />
    <ui:VisualElement style="justify-content: space-between; align-items: stretch; height: 100%; width: 100%; flex-direction: row; padding-left: 56px; padding-right: 56px; padding-top: 56px; padding-bottom: 56px;">
        <ui:VisualElement name="MainContent" style="align-items: center; justify-content: space-between; max-width: 35%;">
            <ui:Label text="Do-o Tower Defense" display-tooltip-when-elided="true" style="font-size: 44px; white-space: normal; -unity-text-align: upper-center;" />
            <ui:VisualElement style="align-items: stretch;">
                <ui:Button text="NEW GAME" display-tooltip-when-elided="true" name="NewGameButton" class="button-style" />
                <ui:Button text="EXIT" display-tooltip-when-elided="true" name="ExitButton" class="button-style" style="font-size: 24px;" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="ImageContainer" style="width: 540px; align-items: center; justify-content: center;">
            <ui:Label text="A beautiful image" display-tooltip-when-elided="true" style="font-size: 100px; white-space: normal; -unity-text-align: upper-center;" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
